function fetchdatafromdatabase() {
    return new Promise((resolve, reject)=> {
        setTimeout(() =>{
        const data = true;

        if (data) {
            resolve (`data fetched successfully`);


        } else {
            reject (`data not found`);

    }
    
    
},3000)
    
});
}

.then ((message )=>{
    console.log(message);
    
})
.catch ((error) =>{
console.log(error);


});
