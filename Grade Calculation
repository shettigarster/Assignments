function studentsgrade(percentage) {
    switch (true) {
        case percentage >=90:
            return 'grade O'
            
            break;
            case percentage >=80:
            return 'grade A'
            
            break;

            case percentage >=60:
            return 'grade B+'
            
            break;
            case percentage >=40&&percentage <=60:
            return 'grade C'

            default:
               return 'grade D' 
            
            break;
    
        
    }
    
}
console.log(studentsgrade(41));



